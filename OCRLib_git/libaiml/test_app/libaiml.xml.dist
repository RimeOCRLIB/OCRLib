  <!-- libaiml options -->
  <options>
    <!-- space delimited list of file patterns to load as aiml files. If you
         need to use a space as part of a pattern escape it with '\' -->
    <file_patterns>aiml/*.aiml</file_patterns>
    
    <!-- file used to store data passed to <gossip> tags (defaults to
        "gossip.txt" if not specified -->    
    <file_gossip>gossip.txt</file_gossip>
    
    <!-- file used to mantain the list of registered users and their variables
     (must be readable+writable) (defaults to "userlist.xml" if not specified) -->
    <user_file>userlist.xml</user_file>
    
    <!-- if this tag is present all combinations of space, tab, carriage
      return and linefeed are replace by a single space (HTML-like behavior) -->
    <trim_blanks/>
    
    <!-- the characters used to separate sentences (every character counts
        here!) (defaults to "?!.;" if not specified) -->
    <sentence_limit>?!.;</sentence_limit>
    
    <!-- the <allow> tags specifies some boolean options -->
    <allow>
      <!-- interpret <javascript> tags (libaiml should have been compiled with
      javascrip support --> 
      <javascript/>
      
      <!-- interpret <system> tags -->      
      <system/>
      
      <!-- allow usage of the "format" attribute on <date/> tag -->
      <dateformat/>
    </allow>
  </options>
</libaiml>
